{
  "version": "0.1.0",
  "configurations": [

    {
      "name": "Launch localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8008",
      "webRoot": "${workspaceFolder}/",
      "runtimeArgs": ["taskset --cpu-list 1 google-chrome"]
    },
    {
      "name": "Launch index.html",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/index.html"
    },
  ]
}